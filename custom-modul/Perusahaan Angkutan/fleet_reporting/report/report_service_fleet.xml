<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <template id="report_service_fleet">
            <t t-call="web.html_container">
                <t t-call="web.internal_layout">
                    <div class="page">
                        <t t-if="tanggal_start and tanggal_finish">
                            <strong><t t-esc="tanggal_start" t-options="{'widget': 'datetime'}"/> - <t t-esc="tanggal_finish" t-options="{'widget': 'datetime'}"/></strong>
                        </t>

                        <t t-if="services">
                            <t t-esc="services" />
                        </t>
                    </div>
                </t>
            </t>
        </template>
    </data>
</odoo>