<odoo>
	<data>
		<record id="continuous_paper_format" model="report.paperformat">
			<field name="name">Continuous Paper Format</field>
			<field name="format">custom</field>
			<field name="page_height">279</field>
			<field name="page_width">241</field>
			<field name="orientation">Landscape</field>
			<field name="margin_top">55</field>
			<field name="margin_bottom">15</field>
			<field name="margin_left">15</field>
			<field name="margin_right">15</field>
			<field name="dpi">90</field>
			<field name="disable_shrinking" eval="True"/>
		</record>

		<record id="f4_landscape" model="report.paperformat">
			<field name="name">F4 Landscape</field>
			<field name="format">custom</field>
			<field name="page_height">330</field>
			<field name="page_width">210</field>
			<field name="orientation">Landscape</field>
			<field name="margin_top">25</field>
			<field name="margin_bottom">15</field>
			<field name="margin_left">5</field>
			<field name="margin_right">5</field>
			<field name="header_spacing">10</field>
			<field name="dpi">90</field>
			<field name="disable_shrinking" eval="True"/>
		</record>
	</data>
</odoo>