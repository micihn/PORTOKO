<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_order_pengiriman" model="ir.sequence">
            <field name="name">Order Pengiriman Sequence</field>
            <field name="code">order.pengiriman.sequence</field>
            <field name="prefix">SO/%(day)s/%(month)s/%(year)s/</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>

    <data noupdate="1">
        <record id="sequence_oper_order" model="ir.sequence">
            <field name="name">Oper Order Sequence</field>
            <field name="code">oper.order.sequence</field>
            <field name="prefix">OP/%(day)s/%(month)s/%(year)s/</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>

    <data noupdate="1">
        <record id="sequence_uang_jalan" model="ir.sequence">
            <field name="name">Uang Jalan Sequence</field>
            <field name="code">uang.jalan.sequence</field>
            <field name="prefix">UJ/%(month)s/%(day)s/%(year)s/</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>

    <data noupdate="1">
        <record id="sequence_konfigurasi_uang_jalan" model="ir.sequence">
            <field name="name">Konfigurasi Uang Jalan Sequence</field>
            <field name="code">konfigurasi.uang.jalan.sequence</field>
            <field name="prefix">KUJ</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
