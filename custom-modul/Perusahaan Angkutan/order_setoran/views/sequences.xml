<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_order_setoran" model="ir.sequence">
            <field name="name">Order Setoran Sequence</field>
            <field name="code">order.setoran.sequence</field>
            <field name="prefix">ST/%(day)s/%(month)s/%(year)s/</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False" />
        </record>
    </data>

    <data noupdate="1">
        <record id="sequence_oper_setoran" model="ir.sequence">
            <field name="name">Oper Setoran Sequence</field>
            <field name="code">oper.setoran.sequence</field>
            <field name="prefix">SOP/%(day)s/%(month)s/%(year)s/</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False" />
        </record>
    </data>

</odoo>
